// main.cpp
#include<vector>

using namespace std;

template <class T>
void printVector(vector<T> &v) {
    for (int i = 0; i < v.size(); i++) {
	cout << "v[" << i << "] = " << v[i] << endl;
	
	// range-based for loop example
	// int index = 0;
	// for (int i : v) {
	// 	cout << "v[" << index << "] = " << i << endl;
	// }

    }
}

int main() {
    vector<int> v;
    for (int i = 0; i < 5; i++) // it could be any reasonable sizeâ€¦
	v.push_back(i);
    
    printVector(v);
    return 0;
}
